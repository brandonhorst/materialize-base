{
  "tasks": {
    "check": "deno fmt && deno lint && deno check",
    "test": "deno test --allow-run --allow-read",
    "coverage": "deno test --allow-run --allow-read --coverage",
    "demo": "deno run --allow-read module.ts test-vault/simple.base --vault-path=test-vault",
    "compile": "deno compile --allow-read --output bin/materialize-base module.ts && chmod +x bin/materialize-base"
  },
  "imports": {
    "@jsep-plugin/regex": "npm:@jsep-plugin/regex@^1.0.4",
    "@std/assert": "jsr:@std/assert@^1.0.15",
    "@std/cli": "jsr:@std/cli@^1.0.23",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@std/testing": "jsr:@std/testing@^1.0.16",
    "@std/yaml": "jsr:@std/yaml@^1.0.10",
    "jsep": "npm:jsep@^1.4.0"
  }
}
